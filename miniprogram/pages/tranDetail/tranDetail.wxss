/* pages/tranDetail/tranDetail.wxss */
.title-wrapper{
  padding-bottom: 10rpx;
  border-bottom: 2rpx solid rgba(128, 128, 128, 0.1);
}
.brief-title-container{
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 20rpx 0;
  border-bottom: 5rpx solid rgba(128, 128, 128, 0.1);
}
.brief-wrapper{
  display: flex;
  flex-direction: column;
  padding: 10rpx;
  border: 5rpx solid rgba(128, 128, 128, 0.3);
  border-radius: 14rpx;
}

.user-wrapper{
  display: flex;
  flex-direction: column;
  align-items: flex-start;
  padding-bottom: 10rpx;
}
.user-container{
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 2rpx solid rgba(128, 128, 128, 0.1);
}
.items-wrapper{
  margin: 10rpx 0;
}
.items-text{
  display: flex;
  justify-content: space-between;
  margin: 20rpx 0;
}
.items-price{
  text-align: end;
}

