.trans-container{
  display: flex;
  flex-direction: column;
}
.tran-wrapper{
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin: 10rpx 0;
  border-bottom: 2rpx solid rgba(128, 128, 128, 0.1);
  padding-bottom: 10rpx;
}
.tran-content{
  flex: 1;
}
.tran-name{
  margin-top: 10rpx;
  text-align: center;
  text-overflow: ellipsis;
}
.status{
  text-align: end;
  margin-top: 20rpx;
}
.setting-arrow{
  width: 40rpx;
  height: 40rpx;
}